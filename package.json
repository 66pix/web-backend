{
  "name": "@66pix/backend",
  "engines": {
    "node": ">=4"
  },
  "version": "1.0.3",
  "description": "The backend service providing authentication for the underlying API",
  "repository": "https://github.com/66pix/backend",
  "license": "UNLICENSED",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "docker-compose run test /srv/www/test.sh",
    "test-watch": "docker-compose run test /srv/www/test-watch.sh",
    "lint": " tslint -c tslint.json --exclude 'typings/**/*' --exclude 'node_modules/**/*' '**/*.ts'",
    "typings-install": "typings install && tsd install",
    "prebuild": "tsconfig .",
    "build": "tsc",
    "start": "pm2 start pm2.json; pm2 logs",
    "development": "pm2 start pm2-dev.json; pm2 logs",
    "coverage-average": "coverage-average coverage/text-summary.txt  --limit 95",
    "precommit": "npm run lint && npm run coverage-average"
  },
  "keywords": [],
  "author": "Michael Robinson",
  "dependencies": {
    "@66pix/api": "^1.1.19",
    "@66pix/email": "0.0.17",
    "@66pix/models": "^0.1.8",
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.0",
    "cheerio": "https://registry.npmjs.org/cheerio/-/cheerio-0.19.0.tgz",
    "convict": "^1.3.0",
    "crypto": "0.0.3",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "jsonwebtoken": "^5.7.0",
    "moment": "^2.13.0",
    "node-constants": "0.0.2",
    "nunjucks": "^2.4.2",
    "pm2": "^1.1.2",
    "ramda": "^0.21.0",
    "resolve": "1.1.7",
    "tsconfig-glob": "^0.4.3"
  },
  "devDependencies": {
    "code": "^2.2.0",
    "coverage-average": "^1.0.4",
    "eslint": "^2.8.0",
    "husky": "^0.11.4",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "require-clean": "^0.1.3",
    "sinon": "^1.17.3",
    "supertest": "^1.2.0",
    "test-plan": "^0.1.1",
    "tsd": "^0.6.5",
    "tslint": "^3.8.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  }
}
