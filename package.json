{
  "name": "@faceleg/backend",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "docker-compose run -e NPM_USERNAME=$NPM_USERNAME -e NPM_PASSWORD=$NPM_PASSWORD -e NPM_EMAIL=$NPM_EMAIL test /srv/www/test.sh",
    "test-watch": "docker-compose run -e NPM_USERNAME=$NPM_USERNAME -e NPM_PASSWORD=$NPM_PASSWORD -e NPM_EMAIL=$NPM_EMAIL test /srv/www/test-watch.sh",
    "lint": "node_modules/.bin/gulp lint",
    "start": "node_modules/.bin/pm2 start pm2.json; ./node_modules/.bin/pm2 logs",
    "development": "node_modules/.bin/pm2 start pm2-dev.json; ./node_modules/.bin/pm2 logs",
    "coverage-average": "./node_modules/.bin/coverage-average coverage/report-summary/text-summary.txt  --limit 95",
    "precommit": "node_modules/.bin/gulp pre-commit && npm run test-single-run && npm run coverage-average",
    "prepush": "npm run test-single-run && npm run coverage-average"
  },
  "keywords": [],
  "author": "Michael Robinson",
  "dependencies": {
    "@faceleg/66pix-api": "^1.0.6",
    "@faceleg/66pix-models": "0.0.3",
    "bluebird": "^2.9.34",
    "body-parser": "^1.13.3",
    "debug": "^2.2.0",
    "errors": "^0.3.0",
    "express": "^4.13.1",
    "express-jwt": "^3.0.1",
    "jsonwebtoken": "^5.0.5",
    "pm2": "^0.14.5",
    "ramda": "^0.17.1",
    "seneca": "^0.6.3"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "coverage-average": "^1.0.3",
    "git-guppy": "^1.0.0",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "gulp-filter": "^3.0.1",
    "gulp-jscs": "^2.0.0",
    "husky": "^0.10.1",
    "istanbul": "^0.3.19",
    "mocha": "^2.3.2",
    "require-clean": "^0.1.1",
    "sinon": "^1.16.1",
    "supertest": "^1.1.0",
    "supertest-as-promised": "^2.0.2",
    "test-plan": "^0.1.1"
  }
}
