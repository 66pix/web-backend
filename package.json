{
  "name": "@66pix/backend",
  "engines": {
    "node": ">=4"
  },
  "version": "1.0.3",
  "description": "The backend service providing authentication for the underlying API",
  "repository": "https://github.com/66pix/backend",
  "license": "UNLICENSED",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "docker-compose run -e NPM_USERNAME=$NPM_USERNAME -e NPM_PASSWORD=$NPM_PASSWORD -e NPM_EMAIL=$NPM_EMAIL test /srv/www/test.sh",
    "test-watch": "docker-compose run test /srv/www/test-watch.sh",
    "lint": "node_modules/.bin/eslint --quiet --ignore-path .gitignore .",
    "start": "pm2 start pm2.json; pm2 logs",
    "development": "pm2 start pm2-dev.json; pm2 logs",
    "coverage-average": "./node_modules/.bin/coverage-average coverage/text-summary.txt  --limit 95",
    "precommit": "npm run lint && npm run coverage-average"
  },
  "keywords": [],
  "author": "Michael Robinson",
  "dependencies": {
    "@66pix/api": "^1.1.14",
    "@66pix/models": "^0.1.4",
    "bluebird": "^3.3.4",
    "body-parser": "^1.15.0",
    "cheerio": "https://registry.npmjs.org/cheerio/-/cheerio-0.19.0.tgz",
    "convict": "^1.1.3",
    "crypto": "0.0.3",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "jsonwebtoken": "^5.7.0",
    "moment": "^2.12.0",
    "node-constants": "0.0.2",
    "pm2": "^1.0.2",
    "ramda": "^0.19.1",
    "resolve": "1.1.7"
  },
  "devDependencies": {
    "code": "^2.1.1",
    "coverage-average": "^1.0.4",
    "eslint": "^2.4.0",
    "husky": "^0.11.4",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "supertest": "^1.2.0",
    "test-plan": "^0.1.1"
  }
}
