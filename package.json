{
  "name": "@faceleg/backend",
  "engines": {
    "node": ">=4"
  },
  "version": "1.0.2",
  "description": "The backend service providing authentication for the underlying API",
  "repository": "https://github.com/faceleg/backend",
  "license": "UNLICENSED",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "docker-compose run -e NPM_USERNAME=$NPM_USERNAME -e NPM_PASSWORD=$NPM_PASSWORD -e NPM_EMAIL=$NPM_EMAIL test /srv/www/test.sh",
    "test-watch": "docker-compose run test /srv/www/test-watch.sh",
    "lint": "node_modules/.bin/eslint --ignore-path .gitignore .",
    "start": "node_modules/.bin/pm2 start pm2.json; ./node_modules/.bin/pm2 logs",
    "development": "node_modules/.bin/pm2 start pm2-dev.json; ./node_modules/.bin/pm2 logs",
    "coverage-average": "./node_modules/.bin/coverage-average coverage/text-summary.txt  --limit 95",
    "precommit": "npm run lint && npm run coverage-average"
  },
  "keywords": [],
  "author": "Michael Robinson",
  "dependencies": {
    "@faceleg/66pix-api": "^1.0.16",
    "@faceleg/66pix-models": "0.0.11",
    "bluebird": "^2.10.2",
    "body-parser": "^1.14.1",
    "debug": "^2.2.0",
    "errors": "^0.3.0",
    "express": "^4.13.3",
    "express-jwt": "^3.1.0",
    "jsonwebtoken": "^5.4.0",
    "node-constants": "0.0.2",
    "pm2": "^0.15.7",
    "ramda": "^0.18.0"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "code": "^1.5.0",
    "coverage-average": "^1.0.4",
    "eslint": "^1.6.0",
    "husky": "^0.10.1",
    "istanbul": "^0.3.22",
    "mocha": "^2.3.3",
    "require-new": "^1.1.0",
    "sinon": "^1.17.1",
    "supertest": "^1.1.0",
    "test-plan": "^0.1.1"
  }
}
